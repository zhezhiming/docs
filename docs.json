{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "appearance": {
    "default": "light"
  },
  "name": "Xpert AI",
  "colors": {
    "primary": "#2D403C",
    "light": "#3D504C",
    "dark": "#1D302C"
